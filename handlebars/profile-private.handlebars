<!DOCTYPE html>
<html lang="en">

{{>base/head font="link"}}

<body>
	<!-- s: root -->
	<div class="root">

		{{>layout/header login=true}}
		
		<!-- grid -->
		<div class="grid">

			<div class="grid-flex">

				<div class="grid-col -nav">
					<div class="nav-fixed">
						{{>layout/nav logged=true}}

						{{>layout/footer footer="small"}}
					</div>
				</div>

				<div class="grid-col -wide">

					<!-- pofile header -->
					{{>component/profile/header profile=people.[1] view=true lock=1}}

					<!-- filter -->
					{{>module/filter type="profile" channel=submenu.profile active="Timeline" position="profile"}}
					
					<!-- feeds -->
					<section class="feed">
						<div class="feed-block">
							<div class="feed-lock">
								<span class="icon-general icon-lock"></span>
								<h4 class="feed-lock-title">This account is private</h4>
								<div class="feed-lock-info">Follow to see their activities</div>
							</div>
						</div>
					</section>

				</div>

				<div class="grid-col -aside">
					{{>layout/sidebar}}
				</div>

			</div>

		</div>

		<!-- notify message -->
		{{>module/panel_alert}}

	</div>
	<!-- e: root -->

	<!-- emoji source -->
	<div id="emoji">
		{{>component/emoji}}
	</div>

	<!-- nav add post -->
	{{>module/nav_add_post}}

{{>base/js}}
</body>
</html>
