<!DOCTYPE html>
<html lang="en">

{{>base/head font="link"}}

<body>
	<!-- s: root -->
	<div class="root">

		{{>layout/header login=true}}
		
		<!-- grid -->
		<div class="grid">

			<div class="grid-flex">

				<div class="grid-col -nav">
					<div class="nav-fixed">
						{{>layout/nav logged=true active="Q&A"}}

						{{>layout/footer footer="small"}}
					</div>
				</div>

				<div class="grid-col -wide">

					<!-- filter -->
					{{>module/filter type="qna" channel=submenu.qna active="Recent"}}

					<!-- qna question -->
					<div class="feed-question">
						<section class="feed">
							<div class="feed-block">
								<div class="feed-content">
									<h1 class="feed-title">
										Apakah mungkin suatu negara memiliki pertumbuhan ekonomi yang tinggi namun tingkat inflasinya rendah?
									</h1>
									<div class="feed-time">5 Answers, 19 hours ago</div>
								</div>
								{{>component/feed/interact_draft question_empty=true}}
								{{>component/feed/inline_form}}
							</div>
						</section>
					</div>

				</div>

				<div class="grid-col -aside">
					{{>layout/sidebar}}
				</div>

			</div>

		</div>

		<!-- notify message -->
		{{>module/panel_alert}}

	</div>
	<!-- e: root -->

	<!-- emoji source -->
	<div id="emoji">
		{{>component/emoji}}
	</div>

	<!-- nav add post -->
	{{>module/nav_add_post}}

{{>base/js}}
</body>
</html>
