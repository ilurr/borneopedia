<!DOCTYPE html>
<html lang="en">

{{>base/head font="link"}}

<body>
	<!-- s: root -->
	<div class="root">

		<div class="welcome-wrap">
			<header class="welcome-logo">
				<h1 class="welcome-img">
					<a href="index.html" role="link">
						<img src="../asset/images/logo-{{lowercase base.title}}-red.svg" alt="{{base.title}}" width="447" height="79">
					</a>
				</h1>
			</header>
			
			<!-- signup -->
			<section class="panel">
				{{>component/title title="Sign Up"}}
				<div class="panel-body">
					{{>module/panel_register}}
				</div>
			</section>

			<section class="welcome-download">
				{{>module/app_widget}}
			</section>

			<section class="welcome-bg">
				<span style="background-image: url('../asset/images/welcome-background.png');"></span>
			</section>
		</div>

	</div>
	<!-- e: root -->

{{>base/js}}
</body>
</html>
