<!-- feed like modal -->
<div id="modalLikedBy" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="modalLikedBy" aria-hidden="true">
	<div class="modal-dialog -small">
		<div class="modal-content">
			<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="icon-close" aria-hidden="true"></span></button>
			<div class="modal-header">
				<h5 class="modal-title">Liked by</h5>
			</div>
			<div class="modal-body">
				<div class="people-block -modal">
					<div class="people-list">
						{{>module/people_list channel=comments index_start=0 index_end=10}}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

<!-- feed vote modal -->
<div id="modalVotedBy" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="modalVotedBy" aria-hidden="true">
	<div class="modal-dialog -small">
		<div class="modal-content">
			<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="icon-close" aria-hidden="true"></span></button>
			<div class="modal-header">
				<h5 class="modal-title">Voted by</h5>
			</div>
			<div class="modal-body">
				<div class="people-block -modal">
					<div class="people-list">
						{{>module/people_list channel=comments index_start=0 index_end=10}}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
